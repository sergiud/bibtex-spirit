    @INPROCEEDINGS{Maynard05a,
    Address        = {Nottingham, UK},
    Author         = {D. Maynard},
    Booktitle      = "{UK e-Science} Programme All Hands Meeting (AHM2005) Workshop on Text Mining, e-Research and Grid-enabled Language Technology",
    Title          = {Benchmarking ontology-based annotation tools for the Semantic Web},
    Year           = {2005},
    abstract	 = {This paper discusses and explores the main issues for evaluating ontology-based annotation tools, a key component in text mining applications for the Semantic Web. Semantic annotation and ontologybased information extraction technologies form the cornerstone of such applications. There has been a great deal of work in the last decade on evaluating traditional information extraction (IE) systems in terms of performance, but such methods are not sufficient for ontology-based systems. Furthermore, usability aspects need to be considered such as scalability, accessibility and interoperability. We outline the main requirements in terms of both usability and performance, proposing possible solutions and using examples of existing tools and resources.},
   url = {http://gate.ac.uk/sale/ahm05/ahm.pdf}

    }

   @INPROCEEDINGS{Maynard05b,
    Address        = {Heraklion, Crete},
    Author         = {D. Maynard and M. Yankova and A. Kourakis and A. Kokossis},
    Booktitle      = {ESWC Workshop "End User Aspects of the Semantic Web")},
    Title          = {Ontology-based information extraction for market monitoring and technology watch},
    Year           = {2005},
    URL		   =
    {http://kmi.open.ac.uk/events/usersweb/papers/03_maynard_final.pdf}
    }


   @INPROCEEDINGS{Maynard06a,
    Address        = {Edinburgh, Scotland},
    Author         = {D. Maynard and W. Peters and Y. Li},
    Booktitle      = "WWW 2006 Workshop on Evaluation of Ontologies for the Web (EON)",
    Title          = {Metrics for Evaluation of Ontology-based Information Extraction},
    Year           = {2006},
    url		   = {http://gate.ac.uk/sale/eon06/eon.pdf},
    abstract	   = {The evaluation of the quality of ontological
    classification is an important part of semantic web technology. Because this
    area is under constant development, it requires improvement and
    standardisation. This paper discusses existing evaluation metrics, and
    proposes a new method for evaluating the ontology population task, which is
    general enough to be used in a variety of situations, yet more precise than
    many current metrics. The paper further describes our first efforts in
    operationalising the evaluation procedure, including the creation of a
    semantically annotated corpus that will function as a test bed for the
    proposed evaluation mechanism, and comparison of different evaluation
    metrics. We conclude that for ontology-based evaluation, a more complex
    mechanism than is traditionally used is preferable. This mechanism aims to
    drive a benchmarking assessment tool for the current state-of-the-art of
    ontology population, and to set a standard for best practice for future
    evaluation of human language technology for the semantic web.}
    }

